#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/7edge/tpvision/grapesjs/packages/cli/dist/node_modules:/home/user/7edge/tpvision/grapesjs/packages/cli/node_modules:/home/user/7edge/tpvision/grapesjs/packages/node_modules:/home/user/7edge/tpvision/grapesjs/node_modules:/home/user/7edge/tpvision/node_modules:/home/user/7edge/node_modules:/home/user/node_modules:/home/node_modules:/node_modules:/home/user/7edge/tpvision/grapesjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/7edge/tpvision/grapesjs/packages/cli/dist/node_modules:/home/user/7edge/tpvision/grapesjs/packages/cli/node_modules:/home/user/7edge/tpvision/grapesjs/packages/node_modules:/home/user/7edge/tpvision/grapesjs/node_modules:/home/user/7edge/tpvision/node_modules:/home/user/7edge/node_modules:/home/user/node_modules:/home/node_modules:/node_modules:/home/user/7edge/tpvision/grapesjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../grapesjs-cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../grapesjs-cli/dist/cli.js" "$@"
fi
